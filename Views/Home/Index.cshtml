                                @Html.TextBoxFor(model => model.CustomerVM.owner_percentage, new { placeholder = "% Ownership", @class = "form-control", id = "ownerPercentage", name = "ownerPercentage", required = "required" })
                                @Html.ValidationMessageFor(model => model.CustomerVM.owner_percentage, "", new { @class = "text-danger" })
                            </div>
