@model CompanyBranding.ViewModel.CustomViewModel
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Form</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <!-- cdn links -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.13.0/css/all.css" integrity="sha384-Bfad6CLCknfcloXFOyFnlgtENryhrpZCe29RTifKEixXQZ38WheV+i/6YWSzkz3V" crossorigin="anonymous" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">
    <link rel="stylesheet" href="~/CSS/style.css">
    <link href="~/Lib/animate/animate.css" rel="stylesheet">




</head>
<body>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    <nav class="navbar sticky-top navbar-expand-lg navbar-light bg-light">
        <div class="container">
            <!-- <a class="navbar-brand" href="#">
                <img class="logo" src="./images/logo_Blue.png" alt="logo">
            </a> -->
        </div>
    </nav>

    <section class="">
        <div class="container">
            <h6 class="mt-4"><i>Please fill this in on a desktop computer or laptop. It has not been designed for use with mobile phones</i></h6>
            <h5>On this webpage you will be able to send all your information and documents so we can start processing your files. Fields mark with " * " are required fields.</h5>
        </div>
    </section>

    <section class="">

        <div class="container">
            @using (Html.BeginForm("Index", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                @Html.AntiForgeryToken()
            <form class="mt-5">
                @Html.ValidationSummary(true)
                <div class="row g-3">
                    <div class="col-lg-6">
                        <div class="form-group row">
                            <label for="name" class="col-form-label">Full Name <span style="color:red"> *</span></label>
                            <div class="">
                                @*<input type="text" class="form-control" id="inputName" placeholder="Full name">*@
                                @Html.TextBoxFor(model => model.CustomerVM.name, new { placeholder = "Full Name", @class = "form-control", id = "name", name = "name", required = "required" })
                                @Html.ValidationMessageFor(model => model.CustomerVM.name, "", new { @class = "text-danger" })

                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group row">
                            <label for="companyName" class=" col-form-label">Company Name<span style="color:red"> *</span></label>
                            <!-- <i>if more than 1, submit seperately</i> -->
                            <div class="">
                                @Html.TextBoxFor(model => model.CustomerVM.company_name, new { placeholder = "Company Name", @class = "form-control", id = "companyName", name = "companyName", required = "required" })
                                @Html.ValidationMessageFor(model => model.CustomerVM.company_name, "", new { @class = "text-danger" })
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group row">
                            <label for="dba" class=" col-form-label">DBA</label>
                            <div class="">
                                @*<input type="text" class="form-control" id="inputDBA" placeholder="DBA">*@
                                @Html.TextBoxFor(model => model.CustomerVM.dba, new { placeholder = "DBA", @class = "form-control", id = "dba", name = "dba" })

                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group row">
                            <label for="email" class=" col-form-label">Email Id<span style="color:red"> *</span></label>
                            <!-- <i>if more than 1, submit seperately</i> -->
                            <div class="">
                                @* <input type="email" class="form-control" id="inputEmail" placeholder="Email">*@
                                @Html.TextBoxFor(model => model.CustomerVM.email, new { placeholder = "Email", @class = "form-control", id = "email", name = "email", required = "required" })
                                @Html.ValidationMessageFor(model => model.CustomerVM.email, "", new { @class = "text-danger" })
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group row">
                            <label for="phone" class=" col-form-label">Phone Number<span style="color:red"> *</span></label>
                            <div class="">
                                @*<input type="tel" id="inputPhone" name="phone" pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}"
        required class="form-control" placeholder="Phone number">*@
                                @Html.TextBoxFor(model => model.CustomerVM.phone, new { placeholder = "Phone Number", pattern = "[0-9]{10}", @class = "form-control", id = "phone", name = "phone", required = "required" })
                                @Html.ValidationMessageFor(model => model.CustomerVM.phone, "", new { @class = "text-danger" })
                            </div>
                        </div>
                    </div>
